SELECT TOP 1000000 solution_id, source_id, ra, ra_error, dec, dec_error, parallax, parallax_error, pm, pmra, pmra_error, pmdec, pmdec_error, pseudocolour, pseudocolour_error, ruwe, phot_g_mean_flux, phot_g_mean_flux_over_error, phot_g_mean_mag, phot_rp_n_obs, phot_rp_mean_flux_over_error, phot_rp_mean_mag, phot_bp_n_obs, phot_bp_mean_flux_over_error, phot_bp_mean_mag, bp_rp, bp_g, g_rp, radial_velocity, radial_velocity_error, rv_nb_transits, vbroad, vbroad_error, grvs_mag, grvs_mag_error, l, b, non_single_star, in_andromeda_survey, teff_gspphot, teff_gspphot_lower, teff_gspphot_upper, logg_gspphot, logg_gspphot_lower, logg_gspphot_upper, distance_gspphot, distance_gspphot_lower, distance_gspphot_upper, ebpminrp_gspphot, ebpminrp_gspphot_lower, ebpminrp_gspphot_upper FROM gaiadr3.gaia_source as g WHERE ruwe IS NOT NULL AND ruwe < 1.2 AND ra IS NOT NULL AND dec IS NOT NULL AND parallax IS NOT NULL AND pmra IS NOT NULL AND pmdec IS NOT NULL AND bp_rp IS NOT NULL AND radial_velocity IS NOT NULL AND in_andromeda_survey = 'false' AND teff_gspphot IS NOT NULL AND logg_gspphot IS NOT NULL AND ra != 'nan' AND dec != 'nan' AND parallax != 'nan' AND pmra != 'nan' AND pmdec != 'nan' AND bp_rp != 'nan' AND radial_velocity != 'nan' AND teff_gspphot != 'nan' AND logg_gspphot != 'nan' AND parallax > 0
